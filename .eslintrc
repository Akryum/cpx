{
    "extends": ["mysticatea", "mysticatea/node"],
    "rules": {
        "no-param-reassign": [2, {"props": false}],
        "node/no-unsupported-features": "off"
    },
    "settings": {
        "node": {
            "convertPath": {
                "src/**/*.js": ["^src/(.+?)$", "$1"]
            }
        }
    }
}
